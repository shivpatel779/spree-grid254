        color: #191919;
        font: 600 16px/31px 'Roboto';
        text-align: center;
        float: left;
      }
      

      .new-rec:hover .cartshare-btn,
      .new-rec .thumbnail:hover .cartshare-btn {
          display: block;
      }
      .hover-state {
          display: none;
          transform: 0.3 ease;
      }
      .cpt-div:hover .show_hover .hover-state,
      .show_hover:hover .hover-state,
      .show_hover .thumbnail:hover .hover-state,
      .show_hover .thumbnail .caption:hover .hover-state {
          display: block;
      }
      .cpt-div:hover .show_hover .default-state,
      .show_hover:hover .default-state,
      .show_hover .thumbnail:hover .default-state,
      .show_hover .thumbnail .caption:hover .default-state {
          display: none;
      }
      /* For testimonial section */
      /* carousel */

      #quote-carousel {
          padding: 0 10px 30px 10px;
          margin-top: 30px;
      }
      /* Control buttons  */

      #quote-carousel .carousel-control {
          background: none;
          color: #222;
          font-size: 2.3em;
          text-shadow: none;
          margin-top: 30px;
      }
      /* Previous button  */

      #quote-carousel .carousel-control.left {
          left: -12px;
      }
      /* Next button  */

      #quote-carousel .carousel-control.right {
          right: -12px !important;
      }
      /* Changes the position of the indicators */

      #quote-carousel .carousel-indicators {
          right: 50%;
          top: auto;
          bottom: 0px;
          margin-right: -19px;
      }
      /* Changes the color of the indicators */

      #quote-carousel .carousel-indicators li {
          background: #c0c0c0;
      }
      #quote-carousel .carousel-indicators .active {
          background: #333333;
      }
      #quote-carousel img {
          width: 250px;
          height: 100px
      }
      .carousel-fade .carousel-inner .item {
          opacity: 0;
          visibility: hidden;
          transition: visibility 0s 2s, opacity 2s linear;
      }
      .carousel-fade .carousel-inner .active {
          opacity: 1;
          visibility: visible;
          transition: opacity 2s linear;
      }
      .carousel-fade .carousel-inner .active.left,
      .carousel-fade .carousel-inner .active.right {
          left: 0;
          opacity: 0;
          z-index: 1;
      }
      .carousel-fade .carousel-inner .next.left,
      .carousel-fade .carousel-inner .prev.right {
          opacity: 1;
      }
      .carousel-fade .carousel-control {
          z-index: 2;
      }
      /* End carousel */

      .item blockquote {
          border-left: none;
          margin: 0;
      }
      .item blockquote img {
          margin-bottom: 10px;
      }
      .item blockquote p:before {
          content: "\f10d";
          font-family: 'Fontawesome';
          float: left;
          margin-right: 10px;
      }

      /**MEDIA QUERIES*/
      /* Small devices (tablets, 768px and up) */

      @media (min-width: 768px) {
          #quote-carousel {
              margin-bottom: 0;
              padding: 0 40px 30px 40px;
          }
      }

      #quote-carousel .item blockquote,
      #quote-carousel .item small {
          border-left: none;
          margin: 0;
          color: #fff;
      }

      /* Small devices (tablets, up to 768px) */
      @media (max-width: 768px) {
          /* Make the indicators larger for easier clicking with fingers/thumb on mobile */

          #quote-carousel .carousel-indicators {
              bottom: -20px !important;
          }
          #quote-carousel .carousel-indicators li {
              display: inline-block;
              margin: 0px 5px;
              width: 15px;
              height: 15px;
          }
          #quote-carousel .carousel-indicators li.active {
              margin: 0px 5px;
              width: 20px;
              height: 20px;
          }
      }


      .animated.fadeIn {
